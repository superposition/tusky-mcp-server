{
  "name": "tusky-mcp-server",
  "version": "0.1.0",
  "description": "MCP server for integrating with the Tusky API based on the Model Context Protocol",
  "type": "module",
  "bin": {
    "tusky-mcp-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "prepublishOnly": "npm run build",
    "test-client": "node test-client.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "claude-desktop",
    "anthropic",
    "mcp-server",
    "template"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "dotenv": "^16.4.5",
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.9",
    "typescript": "^5.3.3"
  }
}
